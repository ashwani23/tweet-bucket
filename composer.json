{
    "name": "ashwani23/tweet-bucket",
    "description": "Small PHP API Client aiming at fetching tweets with hashtag #custserv and are re-tweeted at least once",
    "minimum-stability": "dev",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ashwani Goyal",
            "email": "ashwanigoyal@outlook.in"
        }
    ],
    "require": {
        "phpunit/phpunit": "4.7.x-dev",
        "abraham/twitteroauth": "0.5.1",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "TweetBag\\": "inc"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
